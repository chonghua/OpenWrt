cd /etc/dnsmasq.d;wget --no-check-certificate https://raw.githubusercontent.com/vokins/yhosts/master/hosts;wget --no-check-certifica https://raw.githubusercontent.com/racaljk/hosts/master/hosts -O 123;cat 123 >> hosts | awk '$1 ~ /^[0-9]/ {printf("address=/%s/%s\n", $2,$1)}' hosts > /etc/dnsmasq.d/dnsmasq.adfq;rm -rf 123 hosts;/etc/init.d/dnsmasq restart
